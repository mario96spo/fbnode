<UserControl x:Class="fbnode.UserControl1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:fbnode" d:DataContext="{d:DesignInstance Type=local:RecipesViewModel}"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Loaded="UserControl_Loaded" DataContext="{Binding RelativeSource={RelativeSource Self}}">
    <UserControl.Resources>
        <DataTemplate x:Key="BlowtimeActiveCommandTemplate">
            <StackPanel Orientation="Horizontal" Background="LightGreen">
                <TextBlock Text="{Binding seq_n}" FontWeight="Bold" Width="16" />
                <TextBlock Width="36"/>
                <TextBox x:Name="acc" Text="{Binding acc}" Margin="14,0,0,0" Width="30" Height="20" HorizontalContentAlignment="Center"  LostFocus="param_lost_focus"/>
                <TextBox x:Name="dec" Text="{Binding dec}" Margin="9,0,0,0" Width="30" Height="20" HorizontalContentAlignment="Center"  LostFocus="param_lost_focus"/>
                <TextBox x:Name="speed" Text="{Binding speed}" Margin="9,0,0,0" Width="30" Height="20" HorizontalContentAlignment="Center"  LostFocus="param_lost_focus"/>
                <TextBox x:Name="angle" Text="{Binding angle}" Margin="9,0,0,0" Width="30" Height="20" HorizontalContentAlignment="Center"  LostFocus="param_lost_focus"/>
                <TextBox x:Name="angleaux" Text="{Binding angleaux}" Margin="9,0,0,0" Width="30" Height="20" HorizontalContentAlignment="Center"  LostFocus="param_lost_focus"/>
                <TextBox x:Name="movecount" Text="{Binding movecount}" Margin="9,0,0,0" Width="30" Height="20" HorizontalContentAlignment="Center"  LostFocus="param_lost_focus"/>
                <TextBox x:Name="flip_count" Text="{Binding flip_count}" Margin="9,0,0,0" Width="30" Height="20" HorizontalContentAlignment="Center"  LostFocus="param_lost_focus"/>
                <TextBox x:Name="flip_time" Text="{Binding flip_time}" Margin="9,0,0,0" Width="30" Height="20" HorizontalContentAlignment="Center"  LostFocus="param_lost_focus"/>
                <CheckBox x:Name="blow_active" IsChecked="{Binding blow_active}" Margin="16,0,0,0" Width="30" HorizontalAlignment="Right" Click="blow_active_click"/>
                <TextBox x:Name="blow_time" Text="{Binding blow_time}" Margin="2,0,0,0" Width="30" Height="20" HorizontalContentAlignment="Center"  LostFocus="param_lost_focus"/>
            </StackPanel>
        </DataTemplate>
        <DataTemplate x:Key="BlowtimeActive_Wait_CommandTemplate">
            <StackPanel Orientation="Horizontal" Background="Pink">
                <TextBlock Text="{Binding seq_n}" FontWeight="Bold" Width="16" />
                <TextBlock Text="{Binding wait_time}" Width="20" TextAlignment="Right"/>
                <TextBlock Text="ms" Width="16"/>
